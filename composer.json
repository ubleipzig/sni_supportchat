{
  "name": "ubl/support-chat",
  "description": "A Support Chat for Typo3",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3",
    "CMS",
    "extension",
    "support",
    "chat"
  ],
  "homepage": "https://github.com/ubleipzig/tx-support-chat",
  "license": ["GPL-2.0+"],
  "version": "2.0.0",
  "support": {
    "issues": "https://github.com/ubleipzig/tx-support-chat/issues"
  },
  "require": {
    "typo3/cms": "^7.6.0"
  },
  "authors": [
    {
      "name": "Georg Schoenweger",
      "email": "Georg.Schoenweger@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Andr√© Lahmann",
      "email": "andre.lahmann@uni-leipzig.de",
      "role": "Developer"
    },
    {
      "name": "Frank Morgner",
      "email": "frank.morgner@uni-leipzig.de",
      "role": "Developer"
    },
    {
      "name": "Ulf Seltmann",
      "email": "ulf.seltmann@uni-leipzig.de",
      "role": "Maintainer"
    }
  ],
  "repositories": [{
    "type": "composer",
    "url": "https://composer.typo3.org/"
  }],
  "require-dev": {
    "namelesscoder/typo3-repository-client": "^1.2",
    "nimut/testing-framework": "^1.0",
    "mikey179/vfsStream": "^1.4",
    "phpunit/phpunit": "^4.7 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Ubl\\SupportChat\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ubl\\SupportChat\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/support_chat ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/support_chat"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
